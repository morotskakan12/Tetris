<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

    <GridPane
        stylesheets="/sample/styl.css"
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="sample.innerViewControler"
        fx:id="_innerViewGridCenter"
        alignment="CENTER"


    >
        <VBox

                fx:id="_innerViewVboxCenterGameSeting"
                alignment="CENTER"
        >
            <ImageView
                    fx:id="_imageViewCenterLog"
                    fitHeight="100"
                    fitWidth="200"
                    pickOnBounds="true"
                    preserveRatio="true"
            >
                <image>
                    <Image
                            url="/sample/bild.jpg"
                            backgroundLoading="true"
                    />
                </image>

            </ImageView>

            <Button
                    fx:id="_innerViewButtonCenterGameStart"
                    text="Start"
                    onAction="#onButtonCenterGemeStart"
            ></Button>
            <Button
                    fx:id="_innerViewButtonCenterGemeLevel"
                    text="Level"
                    onAction="#onButtonCenterGemeLevel"
            ></Button>
            <Button
                    fx:id="_innerViewButtonCneterExit"
                    text="Exit"
                    onAction="#onExitClick"
            ></Button>


        </VBox>
    </GridPane>


