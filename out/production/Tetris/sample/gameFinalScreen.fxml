<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<BorderPane
        stylesheets="/sample/styl.css"
        xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.GameFinalScreenControler"
            prefHeight="500.0" prefWidth="300.0"
>
    <center>


            <VBox

                    fx:id="_vboxCenterGameSeting"
                    alignment="CENTER"
            >
                <ImageView
                        fx:id="_imageViewCenterLog"
                        fitHeight="100"
                        fitWidth="200"
                        pickOnBounds="true"
                        preserveRatio="true"
                >
                    <image>
                        <Image
                                url="/sample/bild.jpg"
                                backgroundLoading="true"
                        />
                    </image>

                </ImageView>

                <Button
                        fx:id="_buttonCenterGameStart"
                        text="Start"
                        onAction="#onButtonCenterGemeStart"
                ></Button>
                <Button
                        fx:id="_buttonCenterGemeLevel"
                        text="Level"
                        onAction="#onButtonCenterGemeLevel"
                ></Button>
                <Button
                        fx:id="_buttonCneterExit"
                        text="Exit"
                        onAction="#onExitClick"
                ></Button>

        </VBox>
    </center>
</BorderPane>
