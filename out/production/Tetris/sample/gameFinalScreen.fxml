<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<GridPane
        stylesheets="/sample/styl.css"
        xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.GameFinalScreenControler"
        fx:id="_fanalGridPane"
>



            <VBox

                    fx:id="_vboxCenterGameFinal"
                    alignment="CENTER"
            >
                <ImageView
                        fx:id="_imageViewCenterLog"
                        fitHeight="100"
                        fitWidth="200"
                        pickOnBounds="true"
                        preserveRatio="true"
                >
                    <image>
                        <Image
                                url="/sample/bild.jpg"
                                backgroundLoading="true"
                        />
                    </image>

                </ImageView>

                <Button
                        fx:id="_buttonCenterGameRestart"
                        text="Restart game"
                        onAction="#onButtonCenterGemeRetart"
                ></Button>
                <Button
                        fx:id="_buttonCenterStartPage"
                        text="To startPage"
                        onAction="#onButtonCenterStartPage"
                ></Button>
                <Button
                        fx:id="_buttonCneterExit"
                        text="Exit"
                        onAction="#onExitClick"
                ></Button>

        </VBox>

</GridPane>
